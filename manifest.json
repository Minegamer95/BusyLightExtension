{
    "manifest_version": 3,
    "name": "USB BusyLight Control V2",
    "version": "1.0",
    "description": "Steuert USB BusyLights von Kuando",
    "permissions": [
      "idle"
    ],  
    "action": {
        "default_icon": {
          "232": "icon.png"
        },
        "default_title": "Show BusyLight Control",
        "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "232": "icon.png"
    }
  }
  